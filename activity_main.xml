<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/parent"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="pe.cibertec.permissionsdemo.MainActivity">

   <Button
       android:id="@+id/btn_open_camera"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="Open Camera"

       />
   <Button
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:id="@+id/btn_access_contacts"
       android:text="Acceder a los Contactos"
       android:layout_marginTop="50dp"
       />

   <Button
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:id="@+id/btn_access_contacts_camera"
       android:text="Acceder a la Camara y Contactos"
       android:layout_marginTop="100dp"
       />
</FrameLayout>